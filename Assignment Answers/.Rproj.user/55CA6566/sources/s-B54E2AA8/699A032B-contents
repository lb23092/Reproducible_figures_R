  \FunctionTok{filter}\NormalTok{(}\FunctionTok{complete.cases}\NormalTok{(culmen\_depth\_mm, body\_mass\_g)) }\SpecialCharTok{\%\textgreater{}\%}
  \FunctionTok{select}\NormalTok{(species, culmen\_depth\_mm, body\_mass\_g)}
\end{Highlighting}
\end{Shaded}

